     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "plt.imshow(X_train[1])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "061c7d91-0441-44bd-98dc-ba521001f899",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Sclaling data\n",
    "X_train = X_train/255"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "a43a1a66-eab0-419b-8a6b-d6f8a5d398c1",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Creating CNN\n",
    "from keras import Sequential\n",
    "from keras.layers import Conv2D, MaxPooling2D, Dense, Flatten"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "7e7454fe-5427-43c8-868d-102f5b513653",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = Sequential()\n",
    "\n",
    "model.add(Conv2D(64, kernel_size=(3,3), activation='relu', input_shape=(28,28,1)))\n",
    "model.add(MaxPooling2D((2,2)))\n",
    "\n",
    "#2nd Layer\n",
    "model.add(Conv2D(64, kernel_size=(3,3), activation='relu'))\n",
    "model.add(MaxPooling2D((2,2)))\n",
    "\n",
    "#Flattening the layer\n",
    "model.add(Flatten())\n",
    "\n",
    "model.add(Dense(128, activation='relu'))\n",
    "model.add(Dense(10, activation='softmax'))     #10 as the number of outputs are 0-9\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "6b7da411-ce3b-43a7-9459-e65e45fe6ef8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
